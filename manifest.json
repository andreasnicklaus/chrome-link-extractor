{
  "manifest_version": 3,
  "name": "Link and Email Extractor",
  "description": " Extract Emails and Links from any page and sites that are referenced by the page's sitemap.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content/content.js"]
    }
  ]
}
